        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Pane prefHeight="320.0" prefWidth="447.0">
         <children>
             <Button layoutX="431.0" layoutY="18.0" onAction="#onChooseFileButtonClick" prefHeight="30.0" prefWidth="115.0" text="open file...">
